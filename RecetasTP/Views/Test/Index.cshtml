@model List<DAO.Models.Usuario>

@{
    ViewData["Title"] = "Test Page";
}

<div class="text-center">
    <h1 class="display-4">Test</h1>

    <table>
        <tr>
            <th>Nombre</th>
            <th>Email</th>
            <th>Perfil</th>
            <th>FechaRegistracion</th>
            <th>Acciones</th>
        </tr>
    @foreach (var user in Model)
    {
    <tr>
        <td>@user.Nombre</td>
        <td>@user.Email</td>
        <td>@user.Perfil</td>
        <td>@user.FechaRegistracion</td>
        <td><a href="/test/delete/@user.IdUsuario">Borrar</a></td>
        <td><a href="/test/modify/@user.IdUsuario">Modificar</a></td>

    </tr>
        <br /><br />
    }
    </table>

</div>